<modes>
  <mode name="rus-bhs-morph">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="rus-bhs-disam">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-conv -a"/>
      <program name="vislcg3 --trace --grammar">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
    </pipeline>
  </mode>
  <mode name="rus-bhs-tagger">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
    </pipeline>
  </mode>
  <mode name="rus-bhs-biltrans">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="rus-bhs.autobil.bin"/>
      </program>
    </pipeline>
  </mode>
  <mode name="rus-bhs-lextor">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="rus-bhs.autobil.bin"/>
      </program>
      <program name="lrx-proc -t">
        <file name="rus-bhs.autolex.bin"/>
      </program>
    </pipeline>
  </mode>


  <mode name="rus-bhs-chunker">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="rus-bhs.autobil.bin"/>
      </program>
      <program name="lrx-proc">
        <file name="rus-bhs.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-bhs-rus.rus-bhs.t1x"/>
        <file name="rus-bhs.t1x.bin"/>
      </program>
    </pipeline>
  </mode>
  <mode name="rus-bhs-interchunk">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="rus-bhs.autobil.bin"/>
      </program>
      <program name="lrx-proc">
        <file name="rus-bhs.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-bhs-rus.rus-bhs.t1x"/>
        <file name="rus-bhs.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-bhs-rus.rus-bhs.t2x"/>
        <file name="rus-bhs.t2x.bin"/>
      </program>
    </pipeline>
  </mode>
  <mode name="rus-bhs-postchunk">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="rus-bhs.autobil.bin"/>
      </program>
      <program name="lrx-proc">
        <file name="rus-bhs.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-bhs-rus.rus-bhs.t1x"/>
        <file name="rus-bhs.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-bhs-rus.rus-bhs.t2x"/>
        <file name="rus-bhs.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-bhs-rus.rus-bhs.t3x"/>
        <file name="rus-bhs.t3x.bin"/>
      </program>
    </pipeline>
  </mode>
  <mode name="rus-bhs">
    <pipeline>
      <program name="lt-proc -w">
        <file name="rus-bhs.automorf.bin"/>
      </program>
      <program name="cg-proc -n -w -1">
        <file name="rus-bhs.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="rus-bhs.autobil.bin"/>
      </program>
      <program name="lrx-proc">
        <file name="rus-bhs.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-bhs-rus.rus-bhs.t1x"/>
        <file name="rus-bhs.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-bhs-rus.rus-bhs.t2x"/>
        <file name="rus-bhs.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-bhs-rus.rus-bhs.t3x"/>
        <file name="rus-bhs.t3x.bin"/>
      </program>
      <program name="lt-proc -d">
        <file name="rus-bhs.autogen.bin"/>
      </program>
      
    </pipeline>
  </mode>




</modes>
